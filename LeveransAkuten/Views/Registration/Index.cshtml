@model RegIndexVm

<div class="container">
    <div class="jumbotron">
        <h1>registration</h1>
    </div>
</div>

<div class="container">
    <table>
        <tr>
            <td>
                <div class="container">
                    <div class="jumbotron">
                        <h3>Företag</h3>
                        <form>
                            <div class="form-group">
                                <label asp-for="Company.Username">Lösenord</label>
                                <input asp-for="Company.Username" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.Username"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.Password">Lösenord</label>
                                <input asp-for="Company.Password" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.Password"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.Email">E-mail</label>
                                <input type="email" asp-for="Company.Email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Ange email">
                                <span asp-validation-for="Company.Email"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.PhoneNumber">Lösenord</label>
                                <input asp-for="Company.PhoneNumber" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.PhoneNumber"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.City">Lösenord</label>
                                <input asp-for="Company.City" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.City"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.ZipCode">Lösenord</label>
                                <input asp-for="Company.ZipCode" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.ZipCode"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.StreetAddress">Lösenord</label>
                                <input asp-for="Company.StreetAddress" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.StreetAddress"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Company.">Lösenord</label>
                                <input asp-for="Company.PhoneNumber" type="password" class="form-control" id="exampleInputPassword1" placeholder="lösenord">
                                <span asp-validation-for="Company.PhoneNumber"></span>
                            </div>
                            <button type="submit" class="btn btn-primary">Registrera</button>
                        </form>
                    </div>
                </div>
            </td>
            <td>
                <div class="container">
                    <div class="jumbotron">
                        <h3>Förare</h3>
                        <form>
                            <div class="form-group">
                                <label for="exampleInputEmail1">E-mail</label>
                                <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Ange email">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">Lösenord</label>
                                <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Ange lösenord">
                            </div>
                            <button type="submit" class="btn btn-primary">Registrera</button>
                        </form>
                    </div>
                </div>
            </td>
        </tr>
    </table>
</div>
@section scripts{<link href="~/styles/jockes.css" rel="stylesheet" />}

